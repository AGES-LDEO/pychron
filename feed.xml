---

<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">

  <title>Pychron Blog</title>
  <link href="http://nmgrl.github.io/pychron/blog/"/>
  <link type="application/atom+xml" rel="self" href="http://nmgrl.github.io/pychron/feed.xml"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://nmgrl.github.io/pychron/blog/</id>
  <author>
    <name>Jake Ross</name>
    <email>jirhiker@gmail.com</email>
  </author>

  {% for post in site.posts %}
  <entry>
    <id>http://nmgrl.github.io/pychron{{ post.id }}</id>
    <link type="text/html" rel="alternate" href="http://nmgrl.github.io/pychron{{ post.url }}"/>
    <title>{{ post.title | xml_escape }}</title>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <author>
      <name>Jake Ross</name>
      <uri>http://nmgrl.github.io/pychron</uri>
    </author>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}

</feed>